				SetBuildBuildingsPriceMultiplyPercent(70);			
		}
		if (nGameLevel==eLevelExpert)
		{	

				AddResource(1,15000);
				SetBuildBuildingsTimeMultiplyPercent(30);
				SetBuildBuildingsPriceMultiplyPercent(30);
		 		SetResearchTimeMultiplyPercent(10);
				SetResearchPriceMultiplyPercent(10);		
		}	
		if (nGameLevel==eLevelHardcore)
		{
				AddResource(1,999999);
				AddResource(2,999999);
				SetBuildBuildingsTimeMultiplyPercent(5);
				SetBuildBuildingsPriceMultiplyPercent(5);
		 		SetResearchTimeMultiplyPercent(5);
				SetResearchPriceMultiplyPercent(5);

		}
	EnableUnit("UCS_SPIDER#UCS_SPIDER_SD",false);
	//LockUnit();
	InitAIBuildingsOrders (nGameLevel);
	SetAIVariantResearches(nGameLevel);


}
//--------------------------------------Nothing--------------------//

function void AIWork(int nGameLevel)
{
	++time;
	LockUnit();
	if(time>30)
	{
		time=0;
		AIBuildingsOrders(); 
		Money(nGameLevel);
	}
}

