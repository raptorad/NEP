function void Money(int nGameLevel)
{
	if (nGameLevel==eLevelExpert)
	{

				AddResource(1,5000);
				AddResource(2,5000);

	}
	if (nGameLevel==eLevelHardcore)
	{
				AddResource(1,99999999);
				AddResource(2,99999999);
	}
}

