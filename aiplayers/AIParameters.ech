function void LockUnit()
{
	
	if (GetRace()==eRaceED)
	{
		EnableUnit("ED_STORM",false);
		EnableUnit("ED_AMURSK",false);
		EnableUnit("ED_INTERCEPTOR",false);
		EnableUnit("ED_BOMBER",false);
		EnableUnit("ED_HAN",false);
		EnableUnit("ED_BTTI",false);
		EnableUnit("ED_BTTV",false);
		EnableUnit("ED_BTTVA",false);
		EnableUnit("ED_PAMIR",false);
		EnableUnit("KAUKAZ",false);
		EnableUnit("ED_HT30",false);
		EnableUnit("ED_MBA",false);
		EnableUnit("EDSF_HBATLE",false);
		EnableUnit("EDSF_BATLE",false);
	
		if (GetNumberOfUnitsWithChasis("KAUKAZ_2XPC",1)+GetNumberOfUnitsWithChasis("KAUKAZ_2XL",1)+GetNumberOfUnitsWithChasis("KAUKAZ_J",1)>(GetNumberOfUnitsWithChasis("ED_HT30_4XL",1)+GetNumberOfUnitsWithChasis("ED_HT30_2XHRA",1)+GetNumberOfUnitsWithChasis("ED_HT30_2XJ",1)))
		{
			EnableUnit("ED_HT30_2XHRA",true);
			EnableUnit("ED_HT30_4XL",true);
			EnableUnit("ED_HT30_2XJ",true);
		}
		else
		{
			EnableUnit("ED_HT30_2XHRA",false);
			EnableUnit("ED_HT30_4XL",false);
			EnableUnit("ED_HT30_2XJ",false);
		}
		
		if(GetNumberOfUnitsWithChasis("ED_BTTV_HRA",1)
		+GetNumberOfUnitsWithChasis("ED_BTTV_4XL",1)
		+GetNumberOfUnitsWithChasis("ED_PAMIR_PC",1)
		+GetNumberOfUnitsWithChasis("ED_PAMIR_J",1)
		+GetNumberOfUnitsWithChasis("ED_PAMIR_L",1)
		+GetNumberOfUnitsWithChasis("KAUKAZ_2XPC",1)
		+GetNumberOfUnitsWithChasis("KAUKAZ_2XL",1)
		+GetNumberOfUnitsWithChasis("KAUKAZ_J",1)
		+GetNumberOfUnitsWithChasis("ED_HT30_4XL",1)
		+GetNumberOfUnitsWithChasis("ED_HT30_2XHRA",1)
		+GetNumberOfUnitsWithChasis("ED_HT30_2XJ",1)
		>
		GetNumberOfUnitsWithChasis("ED_MBA_BRA",1)
		+GetNumberOfUnitsWithChasis("ED_MBA_NBRA",1)
		+GetNumberOfUnitsWithChasis("ED_PAMIR_RA",1))
		{
			EnableUnit("ED_PAMIR_RA",true);
			EnableUnit("ED_MBA_NBRA",true);
			EnableUnit("ED_MBA_BRA",true);
		}
		else
		{
			EnableUnit("ED_PAMIR_RA",false);
			EnableUnit("ED_MBA_NBRA",false);
			EnableUnit("ED_MBA_BRA",false);		
		}		
		if(GetNumberOfUnitsWithChasis("ED_BTTI_R",1)
		+GetNumberOfUnitsWithChasis("ED_BTTI_I",1)
		+GetNumberOfUnitsWithChasis("ED_BTTV_AAR",1)
		+GetNumberOfUnitsWithChasis("ED_BTTV_HRA",1)
		+GetNumberOfUnitsWithChasis("ED_BTTV_4XL",1)
		+GetNumberOfUnitsWithChasis("ED_PAMIR_RA",1)
		+GetNumberOfUnitsWithChasis("ED_PAMIR_PC",1)
		+GetNumberOfUnitsWithChasis("ED_PAMIR_J",1)
		+GetNumberOfUnitsWithChasis("ED_PAMIR_L",1)
		+GetNumberOfUnitsWithChasis("KAUKAZ_2XPC",1)
		+GetNumberOfUnitsWithChasis("KAUKAZ_2XL",1)
		+GetNumberOfUnitsWithChasis("KAUKAZ_J",1)
		+GetNumberOfUnitsWithChasis("ED_HT30_4XL",1)
		+GetNumberOfUnitsWithChasis("ED_HT30_2XHRA",1)
		+GetNumberOfUnitsWithChasis("ED_HT30_2XJ",1)
		+GetNumberOfUnitsWithChasis("ED_MBA_BRA",1)
		+GetNumberOfUnitsWithChasis("ED_MBA_NBRA",1)
		>
		+GetNumberOfUnitsWithChasis("ED_INTERCEPTOR_AAR",1)
		+GetNumberOfUnitsWithChasis("ED_INTERCEPTOR_HRA",1)
		+GetNumberOfUnitsWithChasis("ED_STORM_R",1)
		+GetNumberOfUnitsWithChasis("ED_STORM_I",1)
		+GetNumberOfUnitsWithChasis("ED_BOMBER_TORP",1))
		{
			EnableUnit("ED_STORM_R",true);
			EnableUnit("ED_STORM_I",true);
			EnableUnit("ED_INTERCEPTOR_AAR",true);
			EnableUnit("ED_INTERCEPTOR_HRA",true);
			if (GetNumberOfUnitsWithChasis("ED_INTERCEPTOR_AAR",1)
			+GetNumberOfUnitsWithChasis("ED_STORM_R",1)
			+GetNumberOfUnitsWithChasis("ED_STORM_I",1)
			+GetNumberOfUnitsWithChasis("ED_INTERCEPTOR_HRA",1)
			>(GetNumberOfUnitsWithChasis("ED_BOMBER_CBOMB",1)+GetNumberOfUnitsWithChasis("ED_BOMBER_HBOMB",1)))
			{
				EnableUnit("ED_BOMBER_CBOMB",true);
				EnableUnit("ED_BOMBER_HBOMB",true);
				if(GetNumberOfUnitsWithChasis("ED_BOMBER_CBOMB",1)+GetNumberOfUnitsWithChasis("ED_BOMBER_HBOMB",1)>GetNumberOfUnitsWithChasis("EDSF_HDD",1))
				{
					EnableUnit("ED_BOMBER_TORP",true);
					EnableUnit("EDSF_HDD",true);
					if(GetNumberOfUnitsWithChasis("EDSF_HDD",1)>GetNumberOfUnitsWithChasis("EDSF_CC",1))
					{
						EnableUnit("EDSF_CC",true);
						if(GetNumberOfUnitsWithChasis("EDSF_CC",1)>GetNumberOfUnitsWithChasis("EDSF_BC",1))
						{
							EnableUnit("EDSF_BC",true);
							if(GetNumberOfUnitsWithChasis("EDSF_BC",1)>GetNumberOfUnitsWithChasis("EDSF_BS",1))
							{
								EnableUnit("EDSF_BS",true);
							}
							else
							{
								EnableUnit("EDSF_BS",false);
							}
						}
						else
						{
							EnableUnit("EDSF_BC",false);
							EnableUnit("EDSF_BS",false);
						}
					}
					else
					{
						EnableUnit("EDSF_CC",false);
						EnableUnit("EDSF_BC",false);
						EnableUnit("EDSF_BS",false);
					}
				}
				else
				{
					EnableUnit("ED_BOMBER_TORP",false);
					EnableUnit("EDSF_HDD",false);
					EnableUnit("EDSF_CC",false);
					EnableUnit("EDSF_BC",false);
					EnableUnit("EDSF_BS",false);
				}
			}
			else
			{
				EnableUnit("ED_BOMBER_TORP",false);
				EnableUnit("ED_BOMBER_CBOMB",false);
				EnableUnit("ED_BOMBER_HBOMB",false);
				EnableUnit("EDSF_HDD",false);
				EnableUnit("EDSF_CC",false);
				EnableUnit("EDSF_BC",false);
				EnableUnit("EDSF_BS",false);
			}
		}
		else
		{
			EnableUnit("ED_STORM_R",false);
			EnableUnit("ED_STORM_I",false);
			EnableUnit("ED_INTERCEPTOR_AAR",false);
			EnableUnit("ED_INTERCEPTOR_HRA",false);
